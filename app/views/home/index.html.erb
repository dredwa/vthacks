<div class="container-fluid" ng-app="travelApp">
  <div class="panel panel-success">
    <div ng-controller="VacationController">
      <div ng-repeat="vacation in vacations">
        <p>{{ vacation }}</p>
      </div>
      <div ng-repeat="account in accounts">
        <p>{{ account }}</p>
      </div>
    </div>
  </div>
</div>